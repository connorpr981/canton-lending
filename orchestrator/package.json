{
  "name": "lending-orchestrator",
  "version": "0.0.1",
  "description": "Canton Lending Platform CLI Orchestrator",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "cli": "tsx src/index.ts",
    "typecheck": "tsc --noEmit",
    "postinstall": "cd daml.js/canton-lending-0.0.1 && npm install --legacy-peer-deps",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "format": "prettier --write src/",
    "format:check": "prettier --check src/",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "prepare": "husky"
  },
  "lint-staged": {
    "src/**/*.ts": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "dependencies": {
    "@daml/ledger": "2.10.2",
    "@daml/types": "2.10.2",
    "@daml.js/canton-lending": "file:daml.js/canton-lending-0.0.1",
    "commander": "^12.0.0",
    "jsonwebtoken": "^9.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.0.0",
    "@types/jsonwebtoken": "^9.0.0",
    "@types/node": "^20.0.0",
    "@vitest/coverage-v8": "^2.0.0",
    "eslint": "^9.0.0",
    "husky": "^9.0.0",
    "lint-staged": "^15.0.0",
    "prettier": "^3.2.0",
    "tsx": "^4.0.0",
    "typescript": "^5.3.0",
    "typescript-eslint": "^8.0.0",
    "vitest": "^2.0.0"
  }
}
